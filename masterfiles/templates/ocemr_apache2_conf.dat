[%CFEngine BEGIN %]
<VirtualHost *:80>
	ServerName ocemr.$(base_config.net_domain)
	ServerAdmin webmaster@mysite.com

	ErrorLog /var/log/apache2/ocemr/error.log
	CustomLog /var/log/apache2/ocemr/access.log combined

	DocumentRoot /usr/share/ocemr/www

	<Location />
		SetHandler python-program
		PythonHandler django.core.handlers.modpython
		SetEnv DJANGO_SETTINGS_MODULE ocemr.settings
		# For matplotlib:
		SetEnv HOME "/tmp"
		PythonPath "['/usr/share/ocemr/apps'] + sys.path"
		PythonDebug On

		Order Allow,Deny
		Allow from all
	</Location>
	<Location "/media">
		SetHandler None
	</Location>

</Virtualhost>
[%CFEngine END %]
